<div class="row">
  <h1>Bienvenue dans vos espace d'administration</h1>
</div>

<div class="row">
  <h2>Ajouter un quartier</h2>
</div>

<div class="row main">
  <div id="sector">
    <%= form_with model: [:admins, @sector], data: { controller: "sectors" }  do |f| %>
      <%= render 'form_attributes', f:f %>
    <% end %>

    <br><br>

  </div>

  <div class="row">
    <%= render 'search_form' %>
  </div>

  <div>
    <ul id="sectors" data-url="<%= sort_admins_sectors_path %>" >
        <% @sectors.each do |sector| %>
          <%= render "sector", sector: sector %>
        <% end %>
    </ul>
  </div>
</div>
