<div class="row">
  <h1>Bienvenue dans vos espace</h1>
</div>

<div class="row">
  <h2>Ajouter un quartier</h2>
</div>

<div class="row main">
  <div id="sector">
    <%= render 'form' %>
  </div>

  <div class="row">
    <%= render 'search_form' %>
  </div>

  <div>
    <table id="sector_list" class="table table-hover">
      <thead>
        <tr>
          <th scope="col">Position</th>
          <th scope="col">Nom</th>
          <th scope="col">Ville</th>
          <th scope="col"></th>
        </tr>
      </thead>

      <tbody id="sectors" data-url="<%= sort_admins_sectors_path %>" >
        <% @sectors.each do |sector| %>
            <%= render "sector", sector: sector %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
